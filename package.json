{
  "name": "@alienfishconsulting/core",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "nx:reset": "nx reset",
    "nx:new-package": "nx generate @alienfishconsulting/alienfish-core:new-package",
    "coverage:clean": "find . -type d -name coverage -exec rm -rf {} +",
    "build": "nx affected -t build",
    "test": "nx affected -t test",
    "lint": "nx affected -t lint",
    "clean": "nx reset && rm -rf node_modules",
    "changeset": "changeset",
    "ci:build": "nx run-many -t build",
    "ci:test": "nx run-many -t test",
    "ci:lint": "nx run-many -t lint",
    "ci:version": "changeset version",
    "ci:publish": "changeset publish",
    "commit": "cz"
  },
  "devDependencies": {
    "@changesets/cli": "2.28.1",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@eslint/js": "^9.8.0",
    "@nx/eslint": "20.7.1",
    "@nx/eslint-plugin": "20.7.1",
    "@nx/js": "20.7.1",
    "@nx/plugin": "20.7.1",
    "@nx/vite": "20.7.1",
    "@nx/web": "20.7.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "18.16.9",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "husky": "9.1.7",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "20.7.1",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "tsup": "^8.4.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0",
    "verdaccio": "^6.0.5",
    "vite": "^6.0.0",
    "vite-plugin-dts": "~4.5.0",
    "vitest": "^3.1.1"
  },
  "packageManager": "pnpm@10.6.4",
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@nx/devkit": "20.7.1"
  }
}
